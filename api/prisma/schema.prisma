generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Client {
  id        Int    @id @default(autoincrement())
  email     String @unique
  name String
  accountNumber BigInt @unique
  accontBalance Decimal
  creationDate DateTime
  updateDate DateTime
}

